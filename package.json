{
  "name": "gametra",
  "version": "2.0.0",
  "description": "A cloud game driver and automator",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=gametra nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=gametra nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "report:coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/gametra.git"
  },
  "keywords": [
    "gametra"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "files": [
      "test/pausable.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/gametra/issues"
  },
  "devDependencies": {
    "@ostai/eslint-config": "^4.0.0",
    "ava": "^6.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-import": "^2.31.0",
    "nyc": "^17.1.0"
  },
  "dependencies": {
    "easing-functions": "^1.3.0",
    "electron": "^35.0.1",
    "jimp": "^1.6.0",
    "puppeteer": "^24.4.0",
    "ssim.js": "^3.5.0"
  }
}
